{
  "name": "csv2md-shortcut",
  "displayName": "CSV2MD Shortcut",
  "version": "0.1.0",
  "description": "Convert CSV to Markdown Table with shortcut keys",
  "author": "root",
  "packageManager": "yarn@1.22.19",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.8.0",
    "@emotion/cache": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.15.0",
    "plasmo": "^0.80.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "3.6.4",
    "@types/chrome": "0.0.237",
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "clipboardWrite",
      "scripting"
    ],
    "commands": {
      "convert": {
        "suggested_key": {
          "windows": "Alt+J",
          "mac": "Command+J"
        },
        "description": "Convert CSV to Markdown Table"
      }
    }
  }
}
